        </arguments>
      </action>
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>debug_draw_left_collision       = 0
debug_draw_right_collision      = 0
debug_draw_top_collision        = 0
debug_draw_bottom_collision     = 0
debug_draw_horizontal_collision = 0
debug_draw_vertical_collision   = 0
debug_draw_debug_1              = 0
debug_draw_debug_2              = 0


if (debug_draw_vertical_collision) 
   draw_rectangle_colour(x-collisionHorizontal+hspeed,y-(collisionVertical-1),x+collisionHorizontal+hspeed,y+(collisionVertical-1),c_blue,c_blue,c_blue,c_blue,1)

if (debug_draw_horizontal_collision)
   draw_rectangle_colour(x-(collisionHorizontal-1),y+vspeed-(collisionVertical-1),x+(collisionHorizontal-1),y+vspeed+(collisionVertical-1),c_blue,c_blue,c_blue,c_blue,1)
   
if (debug_draw_bottom_collision)
   draw_rectangle_colour(x-(collisionHorizontal-1), y+(collisionVertical+1), x+(collisionHorizontal-1), y+(collisionVertical+1), c_red,c_red,c_red,c_red,1);
   
if (debug_draw_top_collision)
   draw_rectangle_colour(x-(collisionHorizontal-1), y-(collisionVertical+1), x+(collisionHorizontal-1), y-(collisionVertical+1), c_red,c_red,c_red,c_red,1);

if (debug_draw_left_collision)
   draw_rectangle_colour(x-(collisionHorizontal+1), y-(collisionVertical-1), x-(collisionHorizontal+1), y+(collisionVertical-1), c_red,c_red,c_red,c_red,1);

if (debug_draw_right_collision)
   draw_rectangle_colour(x+(collisionHorizontal+1), y-(collisionVertical-1), x+(collisionHorizontal+1), y+(collisionVertical-1), c_red,c_red,c_red,c_red,1);

if (debug_draw_debug_1)
   draw_text(x,y-48,debug1);

if (debug_draw_debug_2)
   draw_text(x,y-64,debug2);
</string>
          </argument>
        </arguments>
      </action>
