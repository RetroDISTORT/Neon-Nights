//health
draw_sprite_ext(spriteHealth,0,x-20,y-32,.5,.5,0,c_white,1)

//draw_rectangle_colour(x-20,y-32,x-20-(health*.5),y-32,c_red,c_red,c_red,c_red,c_red)
</string>
